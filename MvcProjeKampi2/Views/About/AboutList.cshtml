@model List<EntityLayer.Concrete.About>

@{
    ViewBag.Title = "AboutList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


    <table class="table table-hover">
        <br />
        <br />

        <tr>
            <th>#</th>
            <th>Hakkımda Detay1</th>
            <th>Hakkımda Detay2</th>
            <th>Hakkımda Resim1</th>
            <th>Hakkımda Resim2</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <th>@item.AboutId</th>
                <td>@item.AboutDetails1</td>
                <td>@item.AboutDetails2</td>
                <td>@item.AboutImage1</td>
                <td>@item.AboutImage2</td>
            </tr>
        }
    </table>


@*Pop-up*@

@Html.Action("AddAboutPopUp", "About")