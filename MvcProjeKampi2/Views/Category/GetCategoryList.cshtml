@model List<EntityLayer.Concrete.Category>

@{

    ViewBag.Title = "GetCategoryList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";

}

<div class="container">
    <table class="table table-hover">
        <br />
        <br />

        <tr>
            <th>Id</th>
            <th>Ad</th>
            <th>Durum</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CategoryId</td>
                <td>@item.CategoryName</td>
                <td>@item.CategoryStatus</td>
            </tr>
        }
    </table>
    <a href="/Category/AddCategory/" class="btn btn-outline-warning">Yeni Kategori Ekle</a>
</div>

